<section id="contenidogeneral">
    <article>
        <div class="barralateral">
        <div class="botoneslateral">
     <h1>
        <span>
            <img src="/images/imagenperfil.svg">
            <p>Usuario: <%=session.usuario%></p>
            <p>Rol: <%=session.cargo%></p>
        </span>
    </h1>

        <a href="/administracionEncargados">
            Encargados
        </a>

        <a href="/administracionModeradores">
            Moderadores
        </a>
        <a href="/administracionDependencias">
            Dependencias
        </a>

        <a href="/administracionUsuarios">
            Usuarios
        </a>
        <a href="administracionReportes">
            Reportes
        </a>
    </div>
    </div>
    

    <div class="contenido">
        <div class="contenidomoderacion">
            <table>
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>fecha</th>
                        <th>Descripcion</th>
                        <th>Latitud</th>
                        <th>Longitud</th>
                        <th>Apoyos</th>
                        <th>Estatus</th>
                        <th>N denuncias</th>
                        <th>Referencias</th>
                        <th>ID ciudadano</th>
                        <th>Tipo reporte</th>
                        <th>ID dependencia</th>
                    </tr>
                </thead>
                <tbody>
                    <%data.forEach(function(data) {%>
                    <tr>
                        <td><%= data.id_reporte %></td>
                        <td><%= formatDate(data.fecha)%></td>
                        <td><%= data.descripcion %></td>
                        <td><%= data.latitud%></td>
                        <td><%= data.longitud %></td>
                        <td><%= data.n_apoyos %></td>
                        <td><%= data.estatus %></td>
                        <td><%= data.n_denuncias %></td>
                        <td><%= data.referencias%></td>
                        <td><%= data.nombre_ciudadano%></td>
                        <td><%= data.tipo_reporte%></td>
                        <td><%= data.nombre_dependencia%></td>
                        
                        <td><a href=/revisionreportes>Revisar</a></td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
    </article>
</section>



<%- include('partials/_footer')%>
